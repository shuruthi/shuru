import java.util.*;
 
class ReverseString
{
   public static void main(String args[])
   {
      String or, rev = "";
      Scanner in = new Scanner(System.in);
 
      System.out.println("Enter a string to reverse");
      or = in.nextLine();
 
      int length = or.length();
 
      for ( int i = length - 1 ; i >= 0 ; i-- )
         rev = rev + or.charAt(i);
 
      System.out.println("Reverse of string: "+reverse);
   }
}
